{
  "name": "tos",
  "version": "1.0.0",
  "description": "collegeboard pages built in react",
  "main": "index.ts",
  "scripts": {
    "//": "concurrently -k \"npm run serve\" \"npm run test\"",
    "start": "ts-node server/server.ts",
    "test": "jest --watchAll",
    "build": "NODE_ENV=production webpack"
  },
  "author": "Eric",
  "license": "ISC",
  "jest": {
    "roots": [
      "<rootDir>/client",
      "<rootDir>/client/app/tests/"
    ],
    "collectCoverage": true,
    "coverageFormats": [
      "json",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "testURL": "http://localhost",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/client/app/tests/setupTests.ts",
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/client/app/tests/setupTests.ts",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "moduleNameMapper": {
      "\\.(s?css|less)$": "identity-obj-proxy",
      "\\.(jpg|png)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "latest",
    "@fortawesome/fontawesome-svg-core": "latest",
    "@fortawesome/free-solid-svg-icons": "latest",
    "@fortawesome/react-fontawesome": "latest",
    "aws-sdk": "latest",
    "axios": "latest",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "latest",
    "classnames": "latest",
    "crypto": "latest",
    "dotenv": "latest",
    "express": "latest",
    "jsonwebtoken": "latest",
    "jwt-decode": "latest",
    "mime": "latest",
    "moment": "latest",
    "mongoose": "latest",
    "mongoose-unique-validator": "latest",
    "multer": "latest",
    "multer-s3": "latest",
    "numeral": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-dropzone": "latest",
    "react-grid-gallery": "latest",
    "react-hot-loader": "latest",
    "react-input-autosize": "latest",
    "react-loading-screen": "latest",
    "react-redux": "latest",
    "react-router-dom": "latest",
    "react-router-hash-link": "latest",
